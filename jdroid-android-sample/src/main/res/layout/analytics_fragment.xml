<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
			  android:layout_width="match_parent"
			  android:layout_height="match_parent"
			  android:orientation="vertical"
			  android:padding="8dp">

	<Button android:id="@+id/sendExampleEvent"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/sendExampleEvent" />

	<Button android:id="@+id/sendExampleTransaction"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/sendExampleTransaction" />

	<Button android:id="@+id/sendExampleTiming"
			android:layout_width="wrap_content"
			android:layout_height="wrap_content"
			android:text="@string/sendExampleTiming" />

</LinearLayout>
